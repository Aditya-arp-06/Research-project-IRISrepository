import time
def brute_force_solution(c, xr, yr):
    for x in range(-xr, xr+1):
        rhs = x**4 + c
        for y in range(-yr, yr+1):
            if y**3 + x*y == rhs:
                return (x,y)
    return None

fertile_constants_small = [-16, -1, 59, 512]
bounds = [20, 50, 100, 200, 1000,100]
results = {}
for b in bounds:
    times = []
    for c in fertile_constants_small:
        start = time.time()
        sol = brute_force_solution(c, b, b)
        elapsed = time.time() - start
        times.append(elapsed)
    avg = sum(times)/len(times)
    results[b] = avg
for b, avg in results.items():
    print(f"bound={b}: avg_time_ms={avg*1000:.2f}")
